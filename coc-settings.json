{
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "json",
    "rust",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.rootPatterns": [".git", "package.json"],
  "codeLens.enable": true,
  "css.lint.unknownAtRules": "ignore",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.hintSign": " ",
  "diagnostic.infoSign": " ",
  "diagnostic.format": "%message\n[%source:%code]",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "javascriptreact": "javascriptreact",
    "jsx": "javascriptreact",
    "tsx": "typescriptreact",
    "typescriptreact": "typescriptreact"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "hover.floatConfig": {
    "border": true,
    "borderhighlight": "CocFloating",
    "maxHeight": 20,
    "rounded": true,
    "shadow": false
  },
  "hover.previewMaxHeight": 20,
  "hover.target": "float",
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.updateImportsOnFileMove.enable": false,
  "list.maxPreviewHeight": 20,
  "Lua.telemetry.enable": false,
  "notification.preferMenuPicker": true,
  "prettier.printWidth": 120,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "rust-analyzer.cargo.unsetTest": ["core", "ed25519-dalek"],
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.diagnostics.disabled": ["incorrect-ident-case", "macro-error", "missing-unsafe"],
  "rust-analyzer.updates.channel": "nightly",
  "signature.preferShownAbove": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript", "javascript-jsdoc"],
    "typescriptreact": ["javascript", "javascript-jsdoc", "typescript"]
  },
  "suggest.completionItemKindLabels": {
    "class": " ",
    "color": " ",
    "constant": " ",
    "constructor": " ",
    "enum": " ",
    "enumMember": " ",
    "event": " ",
    "field": " ",
    "file": " ",
    "folder": " ",
    "function": " ",
    "interface": " ",
    "keyword": " ",
    "method": " ",
    "module": " ",
    "operator": " ",
    "property": " ",
    "reference": " ",
    "snippet": " ",
    "struct": " ",
    "text": " ",
    "typeParameter": " ",
    "unit": " ",
    "value": " ",
    "variable": " "
  },
  "suggest.defaultSortMethod": "none",
  "suggest.maxCompleteItemCount": 20,
  "suggest.minTriggerInputLength": 2,
  "suggest.removeDuplicateItems": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "tsserver.maxTsServerMemory": 8192,
  "tsserver.watchOptions": {
    "fallbackPolling": "priorityPollingInterval",
    "synchronousWatchDirectory": true,
    "watchDirectory": "fixedPollingInterval",
    "watchFile": "fixedPollingInterval"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enable": false
}
